{
  "extends": "google",
  "env": {
    "browser": true
  },
  "globals": {
    "h": false
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": [2, {"named": "always", "anonymous": "always"}],
    "max-len": [2, 100, {
      "ignoreComments": true,
      "ignoreUrls": true,
      "tabWidth": 2
    }],
    "no-implicit-coercion": [2, {
      "boolean": false,
      "number": true,
      "string": true
    }],
    "no-unused-expressions": [2, {
      "allowShortCircuit": true,
      "allowTernary": false
    }],
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "after-used",
      "argsIgnorePattern": "(^reject$|^_$)",
      "varsIgnorePattern": "(^_$|^h$)"
    }],
    "quotes": [2, "single"],
    "require-jsdoc": 0,
    "valid-jsdoc": 0
  }
}